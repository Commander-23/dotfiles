[user]
    name = cmdr
    email = cmdr@commander23.net

[core]
    editor = nano
    autocrlf = input
    excludesfile = ~/.config/git/ignore

[init]
    defaultbranch=main

[color]
    ui = auto

[credential]
    helper = store
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
